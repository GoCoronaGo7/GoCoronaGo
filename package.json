{
    "name": "gocorongoapp",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "^16.6.1",
        "yarn": "^1.22.10"
    },
    "scripts": {
        "build": "cd client && yarn install && yarn build",
        "postbuild": "cp -r ./client/build ./static/",
        "minify": "swc ./static/build -d ./static/",
        "start": "FLASK_ENV=PRODUCTION PORT=80 authbind gunicorn app:webserver --preload"
    },
    "dependencies": {
        "@swc/cli": "^0.1.55",
        "@swc/core": "^1.2.133"
    }
}
